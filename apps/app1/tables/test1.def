
label My funny table
update admin


#allow update or insert to admin or monitor
#allow update insert to admin, monitor
#allow update, insert | admin | monitor
#allow update insert / admin / monitor + users
allow update, insert, delete ; admin ; monitor + users + !root


=NAME

  type char 1024
  
  unique
  required
  
  options uniq req

=CTIME

  type utime

=SUMA

  type real .4
  update admin
