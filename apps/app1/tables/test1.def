
label My funny table


#allow update or insert to admin or monitor
#allow update insert to admin, monitor
#allow update, insert | admin | monitor
#allow update insert / admin / monitor + users
allow insert; admin ; monitor + users + !root; oper
allow update; admin ; monitor + users + !root
allow read ; all ; oper

=NAME

  type char 1024
  
#  unique
  required

=CTIME

  type utime

=SUMA

  type real .4

=INDEX: TEST

  fields CTIME SUMA
  unique
  
=INDEX: TEST2

  fields NAME CTIME SUMA
  
=DES

  type char 0

=AMOUNT1

  type real .2  

=AMOUNT2

  type real

=AMOUNT3

  type real 10.5
  index
  unique

=AMOUNT4

  type real
  index
  unique

=REF

  type link test2 name
  
  

