  type utime

=SUMA

  type real .4

=INDEX: TEST

  fields CTIME SUMA
  unique
  
=DES

  type char 0

=CNT

  type int
  
=AMOUNT1

  type real .2  

=AMOUNT2

  type real

=AMOUNT3

  type real 10.5
  index
  unique
