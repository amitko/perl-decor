
label My funny table


grant read insert to 999
grant insert update to 1;10

no-copy
no-preview

=NAME

  type char 1024

#  unique
  required

=CTIME

  type utime

=SUMA

  type real .4
  grant read

=INDEX: TEST

  fields CTIME SUMA
  unique

=INDEX: TEST2

  fields NAME CTIME SUMA

=DES

  type char 128
  #password
  web.rows 5

=AMOUNT1

  type real .2
  required

=AMOUNT2

  type real

=AMOUNT3

  type real 10.5
  index
  unique

=AMOUNT4

  type real
  index
  unique
  #ro
  #read_only
  #system
  #web.hidden

=REF

  type link test2 name

#  web.select-where    CNT > ?; 2
  web.combo     %40s=%-15s; NAME; CNT
